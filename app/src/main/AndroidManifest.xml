<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tmnt.coolweather">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Base.ThemeOverlay.AppCompat.Dark.ActionBar">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Action.CityActivity"></activity>
        <activity android:name=".Action.CountyActivity"></activity>
        <activity android:name=".Action.ProvinceActivity"></activity>
        <activity android:name=".Action.ManagerActivity"></activity>

        <service android:name=".Service.MyService"></service>
    </application>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
</manifest>
